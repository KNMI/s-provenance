<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/2017/6/s-prov#"
     xml:base="http://www.semanticweb.org/ontologies/2017/6/s-prov"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
     xmlns:prov="http://www.w3.org/ns/prov#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov">
        <owl:imports rdf:resource="http://purl.org/provone/ontology"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/prov-o-20130430"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/description -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/description">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/format -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/format">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">format or mime-type of the entity</rdfs:comment>
        <rdfs:label xml:lang="en">dcterms:format</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#functionName -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#functionName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Implementation"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Name of the function implementing the Component</rdfs:comment>
        <rdfs:label xml:lang="en">functionName</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#invocationIndex -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#invocationIndex">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Invocation"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:label xml:lang="en">invocationIndex</rdfs:label>
        <prov:definition xml:lang="en">Sequence number of the current invocation</prov:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#mapping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#mapping">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">type of resource mapping for the concrete execution of the workflow; eg. mpi, multiprocessing, apache-storm, etc</rdfs:comment>
        <rdfs:label xml:lang="en">mapping</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#modules -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#modules">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Collection"/>
        <rdfs:comment xml:lang="en">Collection of software modules used within the current execution; eg. python modules with their version indication, &quot;matplotlib==1.4.3&quot;</rdfs:comment>
        <rdfs:label xml:lang="en">modules</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#parameterName -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#parameterName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:label xml:lang="en">parameterName</rdfs:label>
        <prov:definition xml:lang="en">Name associated to a Data element. Used to name a workflow parameter</prov:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#provCluster -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#provCluster">
        <rdfs:comment xml:lang="en">Name of the Cluster to which the Component belongs for runtime provenance analysis</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#queue -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#queue">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Resource"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The queue of a cluster for the scheduling of the workflow job; eg. &quot;jobmanager-pbs&quot;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#resourceName -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#resourceName">
        <rdfs:comment xml:lang="en">Extended Name of the Computational Resource; eg. SCAI_Cluster or LRZ_SMUC</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#resourceType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#resourceType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Resource"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Type of the computational resource access point (if applicable); eg GT5, Unicore etc</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#resourceUrl -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#resourceUrl">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Resource"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">URL of the remote resource where the workflow has been submitted to.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#source -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#source">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Implementation"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Source code or url pointing to an external repository such as a code revision service, eg. github</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#stateLookupTerm -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#stateLookupTerm">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:label xml:lang="en">stateLookupTerm</rdfs:label>
        <prov:definition xml:lang="en">a key literal associated with a Data entity in the StateCollection</prov:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#toStateCollection -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#toStateCollection">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#StateCollection"/>
        <rdfs:comment xml:lang="en">A Data Collection is added to the StateCollection by an invocation</rdfs:comment>
        <rdfs:label xml:lang="en">toStateCollection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#type -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#type">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Component"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Runtime Type of the Component Agent. Multiple types can indicate the computational class and the provenance pattern or metadata schema associated to the Component.</rdfs:comment>
        <rdfs:label xml:lang="en">type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#wasChangedWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#wasChangedWith">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentInstance"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentParameters"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Implementation"/>
        <rdfs:label xml:lang="en">wasChangedWith</rdfs:label>
        <prov:definition xml:lang="en">The relationship indicates that a ComponentInstance changed its ComponentParameters or its Implementation</prov:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#wcount -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#wcount">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:label xml:lang="en">wcount</rdfs:label>
        <prov:definition xml:lang="en">Writing sequence number of a Data entity across invocations</prov:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#workflowId -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#workflowId">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#value"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">System Id of the workflow</rdfs:comment>
        <rdfs:label xml:lang="en">workflowId</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#workflowName -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#workflowName">
        <rdfs:label xml:lang="en">workflowName</rdfs:label>
        <prov:definition xml:lang="en">Common name of the workflow</prov:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#actedOnBehalfOf -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#actedOnBehalfOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentInstance"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Component"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#atLocation -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#atLocation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentInstance"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Resource"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#hadMember -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#hadMember">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#StateCollection"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Workflow"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#DataGranule"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#hadPlan -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#hadPlan">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Component"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#wasAssociatedWith -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Invocation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentInstance"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#wasAttributedTo -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasAttributedTo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Component"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Workflow"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#WFExecution"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Workflow"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Person"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#wasDerivedFrom -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->

    <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Invocation"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/provone/ontology#Process -->

    <rdf:Description rdf:about="http://purl.org/provone/ontology#Process">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Implementation"/>
    </rdf:Description>
    


    <!-- http://purl.org/provone/ontology#Workflow -->

    <rdf:Description rdf:about="http://purl.org/provone/ontology#Workflow">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2017/6/s-prov#Workflow"/>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#Component -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#Component">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:label>Component</rdfs:label>
        <prov:definition>The processing operator of the workflow in its abstract definition. Components are Agents, they delegate to their instances and have Implementations that can change at runtime.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentInstance -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentInstance">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#SoftwareAgent"/>
        <rdfs:comment>This is a SoftwareAgent that acts on behalf of the a Component.
A single Component can map to multiple ComponentInstances as a result of the mapping of the workflow to the underlying resource.
Its location is described as a SystemProcess, which indicates
pid, worker and execution mapping. ComponentInstances can get re-parametrised at runtime or recieve new Implementations.</rdfs:comment>
        <rdfs:label>ComponentInstance</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentParameters -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#ComponentParameters">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:label>ComponentParameters</rdfs:label>
        <prov:definition>Each ComponentInstance and therefore their Invocations are parametrised. Parameters gets assigned at the definition time but can change at runtime.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#Data -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#Data">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Collection"/>
        <rdfs:label>Data</rdfs:label>
        <prov:definition>Any self-contained collection of data produced or used by an Invocation. Each element gets time-stamped and if not volatile, it can refer to
the url of its location.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#DataGranule -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#DataGranule">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:label>DataGranule</rdfs:label>
        <prov:definition>A DataGranule is part of a Data collection and contains contextual metadata related to the specific domain or data format metadata schemas. These can be also user-defined.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#FlowDerivation -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#FlowDerivation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Derivation"/>
        <rdfs:comment>A derviation from an entity b,  wasDerivedFrom(a,b), where b is ingested by the current or any past Invocation.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#Implementation -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#Implementation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Plan"/>
        <rdfs:label>Implementation</rdfs:label>
        <prov:definition>The Plan, or source code of a Component.  It can be represented by the source code itself or by a link or to a versioning system. If applicable it also indicates information about the type of the Component as interpreted by the adopted programming language at the time of the execution. Implementations can change at runtime. Live reimplementation should affect all the ComponentInstaces of a Component. This entity is time-stamped.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#Invocation -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#Invocation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <rdfs:label>Invocation</rdfs:label>
        <prov:definition>The Activity performed by a ComponentInstance over incoming data. Each Invocation can produce more Data.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#Resource -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#Resource">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Location"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2017/6/s-prov#StateCollection -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2017/6/s-prov#StateCollection">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Collection"/>
        <rdfs:label>StateCollection</rdfs:label>
        <prov:definition>The internal state of a ComponentInstance. Contains references
to Data entities that are preserved by Invocations for stateful operations.</prov:definition>
